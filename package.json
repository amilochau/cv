{
  "name": "milochau.cv.client",
  "version": "2.0.0",
  "description": "Milochau CV Client",
  "author": "Antoine Milochau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/cv"
  },
  "homepage": "https://github.com/amilochau/cv#readme",
  "bugs": {
    "url": "https://github.com/amilochau/cv/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "private": true,
  "main": "./src/main.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.19.1",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "vee-validate": "^3.4.14",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.26.7",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@amilochau/eslint-config-vue": "^0.1.0",
    "@amilochau/seo-webpack-plugin": "^0.1.2",
    "@types/numeral": "^2.0.2",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "^4.5.15",
    "sass": "~1.32.8",
    "sass-loader": "^10.1.1",
    "typescript": "^4.4.4",
    "vue-cli-plugin-vuetify": "^2.4.4",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "babel": {
    "presets": [
      [
        "@vue/cli-plugin-babel/preset"
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@amilochau/eslint-config-vue"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "seo": {
    "host": "https://cv.milochau.com",
    "policies": [
      {
        "userAgent": "*",
        "allow": "/"
      }
    ],
    "pages": [
      {
        "relativeUrl": "/",
        "changeFrequency": "daily",
        "priority": 1
      }
    ],
    "languages": [
      {
        "lang": "x-default",
        "relativePrefixUrl": ""
      },
      {
        "lang": "en",
        "relativePrefixUrl": "/en"
      },
      {
        "lang": "fr",
        "relativePrefixUrl": "/fr"
      }
    ]
  }
}
